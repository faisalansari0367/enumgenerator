{
	"name": "code-actions-sample",
	"private": true,
	"displayName": "Code Actions Provider Sample",
	"version": "0.0.2",
	"publisher": "vscode-samples",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"bugs": {
		"url": "https://github.com/Microsoft/vscode-extension-samples/issues"
	},
	"engines": {
		"vscode": "^1.73.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:dart"
	],
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node",
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		}
	},
	"contributes": {
		"commands": [
			{
				"command": "enumgenerator.generateEnumExtension",
				"title": "Generate enum extension"
			},
			{
				"command": "enumgenerator.udpateEnumExtension",
				"title": "Update enum extension"
			}
		]
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./",
		"test": "jest"
	},
	"devDependencies": {
		"@types/jest": "^29.5.5",
		"@types/lodash": "^4.14.199",
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.26.0",
		"jest": "^29.7.0",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	}
}
